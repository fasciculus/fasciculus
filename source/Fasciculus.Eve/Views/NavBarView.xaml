<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModels="clr-namespace:Fasciculus.Eve.ViewModels"
             x:Class="Fasciculus.Eve.Views.NavBarView"
             x:DataType="viewModels:NavBarViewModel">

    <VerticalStackLayout>
        <ImageButton Source="info.png" Command="{Binding GoToCommand}" CommandParameter="//Info" />
        <ImageButton Source="market.png" Command="{Binding GoToCommand}" CommandParameter="//Market" />
        <ImageButton Source="industry.png" Command="{Binding GoToCommand}" CommandParameter="//Industry" />
        <ImageButton Source="planets.png" Command="{Binding GoToCommand}" CommandParameter="//Planets" />
        <ImageButton Source="map.png" Command="{Binding GoToCommand}" CommandParameter="//Map" />
    </VerticalStackLayout>

</ContentView>
