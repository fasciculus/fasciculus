<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0-windows10.0.19041.0</TargetFramework>
    <RootNamespace>Fasciculus.Eve</RootNamespace>
    <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommunityToolkit.Mvvm" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="../Fasciculus.Extensions/Fasciculus.Extensions.csproj" />
    <ProjectReference Include="../Fasciculus.IO/Fasciculus.IO.csproj" />
    <ProjectReference Include="../Fasciculus.Mathematics/Fasciculus.Mathematics.csproj" />
    <ProjectReference Include="../Fasciculus.Maui/Fasciculus.Maui.csproj" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources/EveData">
      <LogicalName>EveData</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources/EveUniverse">
      <LogicalName>EveUniverse</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources/EveNavigation">
      <LogicalName>EveNavigation</LogicalName>
    </EmbeddedResource>
  </ItemGroup>

</Project>
